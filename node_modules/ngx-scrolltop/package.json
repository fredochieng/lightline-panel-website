  "author": "Lukas Bartak <bart@bartweb.cz> (https://github.com/bartholomej)",
  "peerDependencies": {
    "@angular/common": ">=12 <15",
    "@angular/core": ">=12 <15"
  },
  "dependencies": {
    "seamless-scroll-polyfill": "2.2.0",
    "tslib": "^2.3.0"
  },
  "homepage": "https://github.com/bartholomej/ngx-scrolltop",
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/bartholomej/ngx-scrolltop.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/bartholomej/ngx-scrolltop/issues"
  },
  "schematics": "./schematics/collection.json",
  "keywords": [
    "material",
    "angular",
    "ngx",
    "material design",
    "button",
    "animate",
    "scrollTop",
    "scroll to top",
    "goToTop",
