  "typings": "ngx-scrolltop.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./ngx-scrolltop.d.ts",
      "esm2020": "./esm2020/ngx-scrolltop.mjs",
      "es2020": "./fesm2020/ngx-scrolltop.mjs",
      "es2015": "./fesm2015/ngx-scrolltop.mjs",
      "node": "./fesm2015/ngx-scrolltop.mjs",
      "default": "./fesm2020/ngx-scrolltop.mjs"
